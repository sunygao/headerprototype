/** Here we centralize all the fonts. */
@font-face {
  font-family:'CircularSpotifyHead-Black';
  src: url('../static/fonts/head/CircularSpotifyHead-Black_gdi.eot');
  src: url('../static/fonts/head/CircularSpotifyHead-Black_gdi.eot?#iefix') format('embedded-opentype'),
    url('../static/fonts/head/CircularSpotifyHead-Black_gdi.woff') format('woff'),
    url('../static/fonts/head/CircularSpotifyHead-Black_gdi.ttf') format('truetype'),
    url('../static/fonts/head/CircularSpotifyHead-Black_gdi.svg#CircularSpotifyHead-Black') format('svg');
  font-weight: 900;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+000D-25CA;
}


