/**
 * ALL MEDIUM views imported.
 * Media query breakpoint is managed at the app.styl level.
 */

html, body {
	-webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

html {
	font-size: medium-font-size;
}

// VIEWS
@import "main/_med";
@import "pages/index/_med";
@import "pages/about/_med";